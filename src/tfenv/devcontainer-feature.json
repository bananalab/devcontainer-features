{
    "name": "tfenv",
    "id": "tfenv",
    "version": "0.1.1",
    "description": "Install and configure tfenv.",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "v3.00.0"
            ],
            "default": "latest",
            "description": "tfenv version"
        },
        "terraform_version": {
            "type": "string",
            "proposals": [
                "latest"
            ],
            "default": "latest",
            "description": "Default Terraform version"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}